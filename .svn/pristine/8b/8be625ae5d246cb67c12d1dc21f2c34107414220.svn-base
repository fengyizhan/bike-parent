<div class="md-content">
	<div class="index-modal-header">
		<h6 ng-bind="dialog.title"></h6>
		<button class="close md-close" aria-hidden="true" data-dismiss="modal" type="button">×</button>
	</div>
	<div class="modal-body">
		<div class="container-fluid">
			<div class="row">
				<div class="col-md-12">
					<form id="form2" autocomplete="off" class="form-inline">
						<div class="row" style="margin-bottom: 5px;">
							<div class="col-md-12">
								<div class="input-group input-group-sm">
									<span class="input-group-addon dialog-search">消费流水号</span>
									<input type="text" class="form-control" name="id" ng-model="dialog.walletSearchForm.data.id" placeholder="请输入消费流水号" maxlength="11" style="min-width: 120px;">
								</div>
								<div class="input-group input-group-sm">
									<span class="input-group-addon dialog-search">开始时间</span>
									<input type="text" class="form-control" ng-model="dialog.walletSearchForm.data.startTime"
										is-open="dialog.walletSearchForm.startTime.opened" uib-datepicker-popup="yyyy-MM-dd HH:mm:ss" startingDay="1"
										max-date="dialog.walletSearchForm.data.endTime" readonly="readonly" />
									<span class="input-group-btn">
										<button type="button" class="btn btn-default" ng-click="dialog.walletSearchForm.startTime.open($event)">
											<i class="glyphicon glyphicon-calendar"></i>
										</button>
									</span> 
								</div>
								<div class="input-group input-group-sm">
									<span class="input-group-addon dialog-search">结束时间</span>
									<input type="text" class="form-control" ng-model="dialog.walletSearchForm.data.endTime"
										is-open="dialog.walletSearchForm.endTime.opened" uib-datepicker-popup="yyyy-MM-dd HH:mm:ss" startingDay="1"
										min-date="dialog.walletSearchForm.data.startTime" readonly="readonly" />
									<span class="input-group-btn">
										<button type="button" class="btn btn-default" ng-click="dialog.walletSearchForm.endTime.open($event)">
											<i class="glyphicon glyphicon-calendar"></i>
										</button>
									</span> 
								</div>
								<input type="hidden" name="username" ng-model="dialog.walletSearchForm.data.username">
								<button type="submit" class="btn btn-primary btn-sm hidden-xs" 
									ng-click="dialog.walletSearchForm.submit()">
									<span class="glyphicon glyphicon-search"></span>
									<span>查询</span>
								</button>
							</div>
						</div>
						<div class="row hidden-lg" style="margin-bottom: 5px;">
							<div class="col-sm-6">
								<button type="submit" class="btn btn-primary btn-block"
									ng-click="dialog.walletSearchForm.submit()">
									<span class="glyphicon glyphicon-search"></span>
									<span>查询</span>
								</button>
							</div>
						</div>
					</form>
					<pagination2 style="height:350px;" parameters="dialog.walletSearchForm.data" uri="wallet/information/consume/page/" 
						id="pagination" items-per-page="10">
						<table class="table table-bordered table-condensed table-hover" id="walletUserTable">
							<thead>
								<tr>
									<td width="160">消费流水号</td>
									<td width="160">用户名</td>
									<td width="130">消费金额(单位:元)</td>
									<td width="150">创建时间</td>
								</tr>
							</thead>
							<tbody>
								<tr ng-repeat="item in $parent.list track by $index">
									<td ng-bind="item.id"></td>
									<td ng-bind="item.username"></td>
									<td ng-bind="item.money"></td>
									<td ng-bind="item.createTime | date:'yyyy-MM-dd HH:mm:ss'"></td>
								</tr>
							</tbody>
						</table>
					</pagination2>
				</div>
			</div>
		</div>
	</div>
</div>
