package com.tiamaes.bike.common.bean;

import java.util.List;

import com.tiamaes.bike.common.bean.system.Authority;

/**
 * 汽车的基类信息
 * @author lsl
 *
 */
public class Auto extends com.tiamaes.bike.common.bean.system.Authority {
	private static final long serialVersionUID = -2543946967724100582L;
	/**
	 * 车牌号
	 */
	private String carNo;
	/**
	 * 车载机号
	 */
	private String machNo;
	/**
	 * sim卡号
	 */
	private String simCode;
	/**
	 * 车辆自编号
	 */
	private String serialNo;
	/**
	 * vin号（车架号）
	 */
	private String vinCode;

	public String getCarNo() {
		return carNo;
	}

	public void setCarNo(String carNo) {
		this.carNo = carNo;
	}

	public String getMachNo() {
		return machNo;
	}

	public void setMachNo(String machNo) {
		this.machNo = machNo;
	}
	
	public String getSimCode() {
		return simCode;
	}

	public void setSimCode(String simCode) {
		this.simCode = simCode;
	}

	public String getSerialNo() {
		return serialNo;
	}

	public void setSerialNo(String serialNo) {
		this.serialNo = serialNo;
	}

	public String getVinCode() {
		return vinCode;
	}

	public void setVinCode(String vinCode) {
		this.vinCode = vinCode;
	}

	@Override
	public List<? extends Authority> getChildren() {
		return null;
	}

	@Override
	public Boolean getOpen() {
		return null;
	}
}
